[$HOME/.tmux/plugins/tpm]
checkout = git clone https://github.com/tmux-plugins/tpm tpm
    $HOME/.tmux/plugins/tpm/bin/install_plugins
update = git pull
    $HOME/.tmux/plugins/tpm/bin/install_plugins
    $HOME/.tmux/plugins/tpm/bin/update_plugins all
push = git push
status = git status
gc = git gc
